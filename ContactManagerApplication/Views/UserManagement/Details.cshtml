@using Microsoft.AspNetCore.Identity
@model IdentityUser

@{
    ViewData["Title"] = "User Details";
}

<h2>@ViewData["Title"]</h2>

<div>
    <h4>User</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            User Name
        </dt>
        <dd class="col-sm-10">
            @Model.UserName
        </dd>
        <dt class="col-sm-2">
            Email
        </dt>
        <dd class="col-sm-10">
            @Model.Email
        </dd>
    </dl>
</div>

<p>
    
    <a class="btn btn-secondary" asp-controller="UserManagement" asp-action="List">Back to List</a>
</p>

